  <builders>
    <hudson.tasks.Shell>
      <command>export avail=`df -h | grep \/dev\/sda1 | sed &apos;s/\(.*\)G\(.*\)G\(.*\)[G|M].*/\3/g&apos; | sed &apos;s/\ //g&apos; | cut -d . -f 1`
export cutoff_limit=10
# clear ccache is disk space is too low
if [[ $avail -ge $cutoff_limit ]]; then 
     echo &quot;Available disk space is $avail and is greater than $cutoff_limit, ccache not clear&quot;; 
else 
     echo &quot;Cut-off limit for disk space is $cutoff_limit and is greater than available disk space of $avail, clearing ccache&quot;;
     ccache -C
     export avail=`df -h | grep \/dev\/sda1 | sed &apos;s/\(.*\)G\(.*\)G\(.*\)G.*/\3/g&apos;`
     echo &quot;Available disk space after ccache cleared is now $avail&quot;
fi</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>rm -rf ${JOB_NAME}_changelist_*.txt
sudo rm -rf /tmp/*.log
sudo rm -rf $WORKSPACE/framework/il/platform/hypervisor/tardis/temp
echo $JOB_NAME $BUILD_NUMBER $P4_CHANGELIST &gt; $WORKSPACE/${JOB_NAME}_changelist_${P4_CHANGELIST}.txt
echo P4_CHANGESET=$P4_CHANGELIST &gt; $WORKSPACE/changelist.properties
echo BRANCH_NAME=$BRANCH_NAME &gt;&gt; $WORKSPACE/changelist.properties</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># -- ensure content/Avalanche is synced (there is a known issue with git plugin as sometimes it removes Perforce files in this directory)
p4 -u scmbuild -p perforce.spirentcom.com:1666 -c jenkins-${NODE_NAME}-${JOB_NAME}-${BRANCH_NAME}  sync -f //jenkins-${NODE_NAME}-${JOB_NAME}-${BRANCH_NAME}/content/Avalanche/...

# make the version file write able to set version for build
sudo chmod 777 $WORKSPACE/build/il/version

# need to remove information in repos directory and packages directory
# sudo -S rm -rf $WORKSPACE/build/il/repos/*
# sudo -S rm -rf $WORKSPACE/build/il/packages/*

sudo scons target=packageclean
sudo scons target=kernelclean</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>env


# -- set environment variable to build VirtualMini
export VM_PROFILE=all

if [[ $INCLUDE_APPSEC_PKG == &quot;TRUE&quot; ]]; then 
    $WORKSPACE/build/il/scripts/phxbld -j32 yocto_ver=1.5 appliance_yocto_tardis3_mix

    l4l7lxc_storage=/mnt/builds/jenkins/l4l7lxc
    mkdir -p $WORKSPACE/build/il/packages/rawimg
    l4l7lxc_buildno=$(ls ${l4l7lxc_storage}/l4l7lxc*.tgz | awk -F &apos;-&apos; &apos;{print $2}&apos; | sed &apos;s/\.tgz//g&apos;)
    cp -f ${l4l7lxc_storage}/*.tgz $WORKSPACE/build/il/packages/rawimg

    $WORKSPACE/build/il/scripts/phxbld -j32 yocto_ver=1.5 av_version=${l4l7lxc_buildno} appliance_packaging  
    $WORKSPACE/build/il/scripts/phxbld -j32 yocto_ver=1.5 appliance_hypervisor_mix
    $WORKSPACE/build/il/scripts/phxbld -j32 yocto_lxc_ose
else
    $WORKSPACE/build/il/scripts/phxbld -j32 yocto_ver=1.5 appliance_hypervisor_stc_only
fi

sudo rm -rf $WORKSPACE/build/il/bld_*
sudo chmod a+r $WORKSPACE/build/il/packages/rawimg/*.ova

python $WORKSPACE/common/idl/crc.py -f il_idl_crc.txt</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># rm -f il_virtual_$STC_MAJ_MIN_VERSION.tar
# tar cvf il_virtual_$STC_MAJ_MIN_VERSION.tar ./build/il/packages
echo &quot;Finished creating il_virtual_$STC_MAJ_MIN_VERSION.tar&quot;
python $HOME/jenkins/build_cleanup.py --n=30 --file=/mnt/builds/jenkins/il_virtual_yocto-$BRANCH_NAME</command>
    </hudson.tasks.Shell>
