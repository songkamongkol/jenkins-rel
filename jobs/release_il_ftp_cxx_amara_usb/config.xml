    <hudson.plugins.python.Python plugin="python@1.2">
      <command>from ftplib import FTP
import os

output_dir=os.environ['OUTPUT_DIR']
ilhost='ilpkg-03.cal.ci.spirentcom.com'
user='spirent'
passwd='spirent'
c1_usb='/home/spirent/jenkins-rel/workspace/integration_cxx_amara_usb/framework/il/kernel/yocto/Install/usbimg_cxxx-1.0.img.gz'
amara_usb='/home/spirent/jenkins-rel/workspace/integration_cxx_amara_usb/framework/il/kernel/yocto/Install/usb_amara-1.0.img.gz'
ftp = FTP(ilhost)     # connect to host, default port
ftp.login(user,passwd)                     # user anonymous, passwd anonymous@
os.chdir(output_dir)
stc_version = os.environ['STC_MAJ_MIN_VERSION']
buildno = os.environ['BLL_BUILD_NUMBER']
branchid = os.environ['BRANCH_ID']
fullbuildno = stc_version + "." + buildno + branchid
c1_img = "spt-c1_"+ fullbuildno +".img.gz"
amara_img = "ctl3image-" + fullbuildno + ".img.gz"
# ftp.retrbinary('RETR '+amara_usb, open(os.path.basename(amara_usb), 'wb').write)
# ftp.retrbinary('RETR '+c1_usb, open(os.path.basename(c1_usb), 'wb').write)
ftp.retrbinary('RETR '+amara_usb, open(amara_img, 'wb').write)
ftp.retrbinary('RETR '+c1_usb, open(c1_img, 'wb').write)
ftp.quit()</command>
    </hudson.plugins.python.Python>
