  <blockBuildWhenUpstreamBuilding>true</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>${JOB_NAME}</customWorkspace>
  <builders>
    <hudson.tasks.BatchFile>
      <command>set BRANCH_ID=6

REM 0-padding build number
SET FULLVER=00%BLL_BUILD_NUMBER%%BRANCH_ID%
if %BLL_BUILD_NUMBER% gtr 9 SET FULLVER=0%BLL_BUILD_NUMBER%%BRANCH_ID%
if %BLL_BUILD_NUMBER% gtr 99 SET FULLVER=%BLL_BUILD_NUMBER%%BRANCH_ID%
echo FULLVER is %FULLVER%
net use K: /delete
net use K: \\10.40.52.42\home\qabuild Spirent8 /USER:SPIRENTCOM\qabuild
set FILER_DIR=K:\TestCenter\2.00\engineering_builds\GUI_BLL64\%STC_MAJ_MIN_VERSION%.%FULLVER%
echo "Copying raw files from %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64 to %FILER_DIR% ..."
mkdir %FILER_DIR%
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\Model %FILER_DIR%\Model /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\PDUs %FILER_DIR%\PDUs /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\RcmDb %FILER_DIR%\RcmDb /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\Resources %FILER_DIR%\Resources /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\stclib %FILER_DIR%\ stclib /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\Templates %FILER_DIR%\Templates /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\Tcl %FILER_DIR%\Tcl /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\Python %FILER_DIR%\Python /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\STAKCommands %FILER_DIR%\STAKCommands /e /i /y
xcopy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\STAKvirtualenvs %FILER_DIR%\STAKvirtualenvs/e /i /y
copy %PKG_BUILD_OUTPUT_DIR%\bin\Release_x64\*.* %FILER_DIR% /y

</command>
    </hudson.tasks.BatchFile>
  </builders>
