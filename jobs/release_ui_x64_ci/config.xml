    <hudson.tasks.BatchFile>
      <command>python framework\tools\buildUtil\package.py --install TestCenter.Windows.x64.bll.tar.gz --directory=bin/Release_x64
attrib -r C:\jenkins-rel\workspace\release_ui_x64_ci\bin\Release_x64\phxrpc.dll

python framework\tools\buildUtil\package.py --install STAKCommands.beta.Windows.x64.tar.gz --directory=bin/Release_x64 --append</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>python genSln.py</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>pushd C:\Program Files (x86)\Microsoft Visual Studio\2017\Professional\Common7\Tools
call VsDevCmd.bat -arch=x64
popd
msbuild.exe TestCenter.UI.Gen.sln /t:build /p:Configuration=Release /p:Platform=x64
</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>REM cd bin\Release_x64
REM rebase.exe -b 0x12000000 -u . *.dll
REM exit /B 0</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>if %RUN_BLL_UI_UNITTEST% == true (
cd bin\Release_x64
python ..\..\framework\tools\testRunner\testRunner.py --runInParallel=8 --ui --archive
rmdir /Q /S unit_test_report
mkdir unit_test_report
copy *.html unit_test_report
echo move *_CoverageReport unit_test_report
)</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>cd bin\Release_x64
del TestCenter*.tar.gz
if %BUILD_PACKAGE_BUILD% == true (
xcopy /Y /R /D ..\..\scm\tools\devexpress .
xcopy /Y /R /D ..\..\scm\tools\dundas .
xcopy /Y /R /D ..\..\scm\tools\janus .
python ..\..\framework\tools\buildUtil\package.py --package=TestCenter 
python ..\..\framework\tools\buildUtil\package.py --package=STAKCommands.beta
copy TestCenter.Windows.x64.tar.gz TestCenter.Windows.x64.no_version.tar.gz
python ..\..\framework\tools\buildUtil\setver.py --package=TestCenter.Windows.x64.tar.gz %STC_MAJ_MIN_VERSION% %P4_CHANGELIST%
)</command>
    </hudson.tasks.BatchFile>
