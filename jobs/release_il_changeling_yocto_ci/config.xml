    <hudson.tasks.Shell>
      <command>rm -rf ${JOB_NAME}_changelist_*.txt
sudo rm -rf /tmp/*.log
sudo rm -rf $WORKSPACE/framework/il/platform/hypervisor/tardis/temp
sudo rm -rf $WORKSPACE/framework/il/platform/hypervisor/tardis/tardis.tgz
echo $JOB_NAME $BUILD_NUMBER $P4_CHANGELIST &gt; $WORKSPACE/${JOB_NAME}_changelist_${P4_CHANGELIST}.txt
echo P4_CHANGESET=$P4_CHANGELIST &gt; $WORKSPACE/changelist.properties
echo BRANCH_NAME=$BRANCH_NAME &gt;&gt; $WORKSPACE/changelist.properties</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#remove the previous build
#rm -rf $WORKSPACE/build/il/packages/central/testcenter*.*
sudo scons target=packageclean

# -- Extract l4l7 legacy content for blink
l4l7blink_filename=$(basename `ls ${WORKSPACE}/blink*.tgz`)
tar xvfz ${WORKSPACE}/${l4l7blink_filename} -C ${WORKSPACE}/content/traffic/wawr/il

# -- Extract l4l7 legacy content for aonic
l4l7aonic_filename=$(basename `ls ${WORKSPACE}/aonic*.tgz`)
tar xvfz ${WORKSPACE}/${l4l7aonic_filename} -C ${WORKSPACE}/content/traffic/wawr/il</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># export CCACHE_DISABLE=1 
export SCONSFLAGS=&quot;-j32 ccache=1 distcc=1&quot;
# $WORKSPACE/build/il/scripts/phxbld centralboot_changeling
scons target=raw_yocto arch=yocto_i686

$WORKSPACE/build/il/scripts/phxbld -j32 yocto_ver=1.5 c3_hypervisor
$WORKSPACE/build/il/scripts/phxbld -j32 yocto_ver=1.5 centralboot_changeling_yocto2
sudo scons target=c3_pkg_yocto_vm packaging=1 yocto_ver=1.5 strict=no

export PAD_BUILD_NUMBER=`printf &apos;%04i&apos; $BUILD_NUMBER`
export PAD_BUILD_NUMBER=${PAD_BUILD_NUMBER:(-4)} # reversion_il.sh cannot take build number greater than 4 digits

sudo scons target=reversion_il il_version=$RELEASE_NUM.$PAD_BUILD_NUMBER av_version=$RELEASE_NUM.$PAD_BUILD_NUMBER
</command>
    </hudson.tasks.Shell>
    <hudson.plugins.python.Python plugin="python@1.3">
      <command># yocto images are named with _yoctovm_ and need to be renamed until this is fixed

import glob
import os
import string

fsimg_dir = os.path.join(os.environ[&apos;WORKSPACE&apos;], &apos;build&apos;, &apos;il&apos;, &apos;packages&apos;, &apos;fsimg&apos;)
os.chdir(fsimg_dir)
ctl_images = glob.glob(&apos;*yoctovm*&apos;)

for file in ctl_images:
	newname = string.replace(file , &apos;_yoctovm&apos;, &apos;&apos;)
	print newname
	os.rename(file ,newname)
</command>
    </hudson.plugins.python.Python>
