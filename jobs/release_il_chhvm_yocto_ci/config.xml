  <builders>
    <hudson.tasks.Shell>
      <command>rm -rf ${JOB_NAME}_changelist_*.txt
sudo rm -rf /tmp/*.log
sudo rm -rf $WORKSPACE/framework/il/platform/hypervisor/tardis/temp
echo $JOB_NAME $P4_CHANGELIST $BUILD_NUMBER &gt; $WORKSPACE/${JOB_NAME}_changelist_${P4_CHANGELIST}.txt
echo P4_CHANGESET=$P4_CHANGELIST &gt; $WORKSPACE/changelist.properties
echo BRANCH_NAME=$BRANCH_NAME &gt;&gt; $WORKSPACE/changelist.properties</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>sudo chmod 777 $WORKSPACE/build/il/version
sudo scons target=packageclean
sudo scons target=kernelclean</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># The file 'chassisController.tgz' is now synced from 
# Perforce (integration branch) to the root of the 
# workspace. The line below copy the file to the 
# expected location before starting the build.
mkdir -p $WORKSPACE/build/il/packages/rawimg
cp $WORKSPACE/chassisController.tgz $WORKSPACE/build/il/packages/rawimg
chmod a+rw $WORKSPACE/build/il/packages/rawimg/chassisController.tgz</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>env

#$WORKSPACE/build/il/scripts/phxbld -j32 c3_y3
#$WORKSPACE/build/il/scripts/phxbld -j32 yocto_ver=1.0 c3_hypervisor
$WORKSPACE/build/il/scripts/phxbld -j16 yocto_ver=$YOCTO_VERSION c3_y3_mix
$WORKSPACE/build/il/scripts/phxbld -j16 yocto_ver=$YOCTO_VERSION c3_hypervisor</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># clean up old build output
python $HOME/jenkins/build_cleanup.py --n=50 --file=/mnt/builds/jenkins/il_chhvm_yocto-$BRANCH_NAME
</command>
    </hudson.tasks.Shell>
  </builders>
