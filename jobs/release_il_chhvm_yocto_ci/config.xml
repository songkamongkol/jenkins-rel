    <org.jenkinsci.plugins.postbuildscript.PostBuildScript plugin="postbuildscript@0.17">
      <buildSteps>
        <hudson.tasks.Shell>
          <command># copy the build output to storage server
export STORAGE_DIR=/mnt/builds/jenkins/il_chhvm_yocto-$BRANCH_NAME/build-$P4_CHANGELIST

if [ ! -d "$STORAGE_DIR" ]; then
  mkdir -p $STORAGE_DIR
  mkdir -p $STORAGE_DIR/fsimg
fi

cp -r $WORKSPACE/build/il/packages/bundle $STORAGE_DIR
cp $WORKSPACE/build/il/packages/fsimg/*chassis_hypervisor* $STORAGE_DIR/fsimg
cp $WORKSPACE/build/il/packages/fsimg/yocto_version $STORAGE_DIR/fsimg
cp -r $WORKSPACE/build/il/packages/rawimg $STORAGE_DIR
cp -r $WORKSPACE/build/il/packages/fsimg/*chassis_installer* $STORAGE_DIR/fsimg</command>
        </hudson.tasks.Shell>
      </buildSteps>
      <scriptOnlyIfSuccess>true</scriptOnlyIfSuccess>
      <scriptOnlyIfFailure>false</scriptOnlyIfFailure>
      <markBuildUnstable>false</markBuildUnstable>
    </org.jenkinsci.plugins.postbuildscript.PostBuildScript>
