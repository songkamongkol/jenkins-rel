      <script plugin="script-security@1.12">
        <script>import hudson.model.*

if(manager.logContains(".*Aborting regression as.*")) {
    manager.addWarningBadge("Gracefully Aborted")
    manager.createSummary("warning.gif").appendText("&lt;i&gt;This run was aborted because it will be later triggered by upStream job&lt;i&gt;", false, false, false, "orange")
    // manager.buildUnstable()
    //manager.listener.logger.println("Marking build unstable") 
    //manager.build.setResult(hudson.model.Result.UNSTABLE)
   manager.build.@result = hudson.model.Result.UNSTABLE
}

if(manager.logContains(".*crc mismatch.*")) {
    manager.addWarningBadge("Gracefully Aborted due to crc mismatch in BLL and IL components")
    manager.createSummary("warning.gif").appendText("&lt;i&gt;This run was aborted due to crc mismatch between BLL and IL component&lt;i&gt;", false, false, false, "orange")
    // manager.buildUnstable()
    //manager.listener.logger.println("Marking build unstable") 
    //manager.build.setResult(hudson.model.Result.UNSTABLE)
   manager.build.@result = hudson.model.Result.UNSTABLE
}

</script>
        <sandbox>false</sandbox>
        <classpath/>
      </script>
