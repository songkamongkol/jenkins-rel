    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.BatchFile>
      <command>p4 revert //...</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>rmdir /q /s regression\il_img
mkdir regression\il_img\</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>python regression\jenkins\copy_artifacts.py release_il_changeling_yocto_ci \\storage.cal.ci.spirentcom.com\builds\jenkins\il_changeling_yocto-release regression\il_img --clean-target

python regression\jenkins\compare_crc.py --first bll_idl_crc.txt --second regression\il_img\il_idl_crc.txt</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>python regression/killProcess.py --keywords "equipServer tclsh TestCenterSession tshark"

REM install BLL/IL

python regression/stcInstall.py --stc-install-dir="%INSTALL_DIR%" --stc-package=TestCenter.Windows.bll.tar.gz --chassis="172.20.64.11 2" --slotWaitTimeInSec 600 --fw-image-dir=regression\il_img --stc-res-package=STAKCommands.beta.Windows.tar.gz

REM python regression/stcInstall.py --stc-install-dir="%INSTALL_DIR%" --stc-package=TestCenter.Windows.bll.tar.gz --chassis="172.20.64.4 1 172.20.64.3 1" --slotWaitTimeInSec 600 --fw-image-dir=regression\il_img --stc-res-package=STAKCommands.beta.Windows.tar.gz

</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>REM run test

tclsh regression\util\unittest\all.tcl


python regression/run.py --vm-user-id=ci_regression --stc-pkg-dir=C:/jenkins/workspace/release_regression_changeling_ci/stc_install_dir  -m '%FILTER%' -n 3 --archive-tmpdir --keep-all-tmpdir --basetemp=c:/pytest_tmp --hw='{\"BLINK\":[(\"//172.20.64.11/8/1\", \"//172.20.64.11/8/2\"),(\"//172.20.64.11/8/3\", \"//172.20.64.11/8/4\"), (\"//172.20.64.11/8/7\", \"//172.20.64.11/8/8\")], \"TBIRD\":[(\"//172.20.64.11/11/1\", \"//172.20.64.11/11/2\"),(\"//172.20.64.11/11/5\", \"//172.20.64.11/11/6\"), (\"//172.20.64.11/11/9\", \"//172.20.64.11/11/10\"), (\"//172.20.64.11/11/13\", \"//172.20.64.11/11/14\")]}'

REM python regression/run.py --vm-user-id=ci_regression --stc-pkg-dir=C:/jenkins/workspace/release_regression_changeling_ci/stc_install_dir  -m '%FILTER%' -n 3 --archive-tmpdir --keep-all-tmpdir --basetemp=c:/pytest_tmp --hw='{\"BLINK\":[(\"//172.20.64.4/2/1\", \"//172.20.64.4/2/2\"),(\"//172.20.64.4/2/3\", \"//172.20.64.4/2/4\"), (\"//172.20.64.4/2/7\", \"//172.20.64.4/2/8\")], \"TBIRD\":[(\"//172.20.64.3/1/1\", \"//172.20.64.3/1/2\"),(\"//172.20.64.3/1/5\", \"//172.20.64.3/1/6\"), (\"//172.20.64.3/1/9\", \"//172.20.64.3/1/10\"), (\"//172.20.64.3/1/13\", \"//172.20.64.3/1/14\")]}'

python regression/killProcess.py --keywords "equipServer tclsh TestCenterSession tshark"

exit /B 0</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>python regression\jenkins\junitEmail.py --junit result.junit.xml --output-file email.properties
python regression\jenkins\removeResults.py --location %WORKSPACE%\regression</command>
    </hudson.tasks.BatchFile>
