    <hudson.tasks.Fingerprinter>
      <targets>TestCenterDebug.Windows*.tar.gz, ${BUILD_OUTPUT_DIR}/UnitTestLogs.tar.gz</targets>
    </hudson.tasks.Fingerprinter>
