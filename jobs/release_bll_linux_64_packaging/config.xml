c:\python27\python %WORKSPACE%\Build_Scripts\Scripts\ci_run_jenkins_steps.py --createpropfile=no --runsetver=no --runcopyeattomartin=no --updatepkgindextcl=yes --updatespirenttestcentertcl=yes --updatestclibtcl=yes --updategemspecversion=yes --runcimaketarballs=no --updateilversionxml=yes

c:\python27\python %WORKSPACE%\Build_Scripts\Scripts\ci_run_jenkins_steps.py --createpropfile=yes --runcimaketarballs=yes --runsetver=no --runcopyeattomartin=no</command>
    </hudson.tasks.BatchFile>
    <hudson.plugins.python.Python plugin="python@1.2">
      <command>import urllib2

# -- get the filename for latest install_stcapi*.py from main Jenkins server
stc_restapi_job_url = 'http://jenkins.cal.ci.spirentcom.com:8080/job/stcapi_rest_api/lastSuccessfulBuild/api/xml?xpath=/*/artifact[1]/fileName'
html = urllib2.urlopen(stc_restapi_job_url)
install_stcapi_filename = html.read()
install_stcapi_filename = install_stcapi_filename.replace('&lt;fileName&gt;', '')
install_stcapi_filename = install_stcapi_filename.replace('&lt;/fileName&gt;', '')
print "Filename: %s" % install_stcapi_filename

# -- Download stcapi installer python script
stc_restapi_py = 'http://jenkins.cal.ci.spirentcom.com:8080/job/stcapi_rest_api/lastSuccessfulBuild/artifact/deployment/' + install_stcapi_filename
response = urllib2.urlopen(stc_restapi_py )
html = response.read()
local_restapi_py = open(install_stcapi_filename,'wb')
local_restapi_py.write(html)
local_restapi_py.close()</command>
    </hudson.plugins.python.Python>
