      <command>del /f /q BUILD_*.txt
echo extracting TestCenter.Linux.x64.full.tar.gz file from job %COPYARTIFACT_BUILD_NUMBER_BLL_LINUX%
rem python framework\tools\buildUtil\package.py --install TestCenter.Linux.x64.full.tar.gz

rd /s /q Spirent_TestCenter_Application_Linux_Server

7z x TestCenter.Linux.x64.full.tar.gz &gt; 7zextract_TestCenter.Linux.x64.full.tar.txt
