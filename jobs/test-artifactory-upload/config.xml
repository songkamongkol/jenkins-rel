      &quot;target&quot;: &quot;stc/stca/&quot;,
      &quot;props&quot;: &quot;name=${STCA_NAME};version=${STC_MAJ_MIN_VERSION}.${BLL_BUILD_NUMBER}${BRANCH_ID};category=${CATEGORY};application_id=${APP_ID};description=&quot;${STCA_DESCRIPTION}&quot;;os=&quot;Linux - 32 bit&quot;;format=&quot;deb&quot;&quot;,
