    </hudson.tasks.BatchFile>
    <hudson.plugins.python.Python plugin="python@1.3">
      <command>import os
import glob
import re

files = glob.glob("*il_testmodule_contents*changelist*.txt")
# files = glob.glob("BLL_*.txt")
changelistfile = files[0]
changelist = re.sub(r'.txt$', "", changelistfile )
changelist = re.sub(r'il_testmodule_contents_changelist_', "", changelist )
build_number = os.environ['BUILD_NUMBER']
print "IL Changelist is " + changelist
il_changelist_file='IL_' + changelist + '.txt'
f = open(il_changelist_file, 'w')
f.write("\nCHANGELIST=" + changelist + " " + build_number)
f.close</command>
    </hudson.plugins.python.Python>
